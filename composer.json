{
    "name": "mezzio/mezzio-router",
    "description": "Router subcomponent for Mezzio",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mezzio",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "homepage": "https://mezzio.dev",
    "support": {
        "docs": "https://docs.mezzio.dev/mezzio-router/",
        "issues": "https://github.com/mezzio/mezzio-router/issues",
        "source": "https://github.com/mezzio/mezzio-router",
        "rss": "https://github.com/mezzio/mezzio-router/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-develop": "2.3-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "fig/http-message-util": "^1.1.2",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "psr/http-message": "^1.0.1",
        "webimpress/http-middleware-compatibility": "^0.1.1"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~1.0.0",
        "malukenho/docheader": "^0.1.5",
        "phpunit/phpunit": "^6.0.8 || ^5.7.15"
    },
    "suggest": {
        "mezzio/mezzio-aurarouter": "^1.0 to use the Aura.Router routing adapter",
        "mezzio/mezzio-fastroute": "^1.2 to use the FastRoute routing adapter",
        "mezzio/mezzio-laminasrouter": "^1.2 to use the laminas-router routing adapter"
    },
    "autoload": {
        "psr-4": {
            "Mezzio\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MezzioTest\\Router\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@license-check",
            "@cs-check",
            "@test"
        ],
        "upload-coverage": "coveralls -v",
        "cs-check": "phpcs --colors",
        "cs-fix": "phpcbf --colors",
        "license-check": "docheader check src/ test/",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "replace": {
        "zendframework/zend-expressive-router": "self.version"
    }
}
